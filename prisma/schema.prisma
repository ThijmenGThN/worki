// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("PRISMA_MONGODB_AUTH_URL")
}

model shifts {
  id Int @id @map("_id")
  email String
  timestamp Int
}
